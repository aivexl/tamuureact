{
  "name": "tamuu",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@tamuu/web",
    "dev:api": "turbo run dev --filter=@tamuu/api",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=@tamuu/web",
    "build:api": "turbo run build --filter=@tamuu/api",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "deploy": "turbo run deploy",
    "deploy:web": "turbo run deploy --filter=@tamuu/web",
    "deploy:api": "turbo run deploy --filter=@tamuu/api"
  },
  "devDependencies": {
    "turbo": "^2.0.0"
  }
}